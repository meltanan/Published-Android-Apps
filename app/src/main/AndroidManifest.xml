<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.education.mohamed.hw2">

    <application
        android:allowBackup="true"
        android:icon="@drawable/expenses2"
        android:label="@string/app_name"

        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddExpense"
            android:screenOrientation="portrait"/>
        <activity android:name=".EdditActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ShowExpense"
            android:screenOrientation="portrait"/>
        <activity android:name=".DeleteActivity"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>